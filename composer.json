{
  "name": "prabowosd/laravel-menus",
  "description": "A menu builder for Laravel",
  "keywords": ["laravel", "menu"],
  "license": "MIT",
  "authors": [
    {
      "name": "Satrio Dwi Prabowo",
      "email": "prabowo.sd@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/prabowosd/laravel-collective-html"
    }
  ],
  "require": {
    "php": "^7.3|^8.0|^8.1|^8.2",
    "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
    "illuminate/config": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
    "illuminate/view": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
    "prabowosd/laravel-collective-html": "dev-main"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5|^9.0|^10.0",
    "mockery/mockery": "^1.4",
    "orchestra/testbench": "^6.2|^7.0|^8.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Nwidart\\Menus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nwidart\\Menus\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.0-dev"
    },
    "laravel": {
      "providers": [
        "Nwidart\\Menus\\MenusServiceProvider"
      ],
      "aliases": {
        "Menu": "Nwidart\\Menus\\Facades\\Menu"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
